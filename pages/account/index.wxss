page{background:url('data:image/jpeg;base64,')no-repeat top center; background-size:100% 100%;}
page{ height:auto; }
.container{  height:100%; overflow-y:scroll; -webkit-overflow-scrolling:touch; box-sizing:border-box; position:relative; padding-bottom:30rpx;}
.container .qrcodeCon{ background:#fff; color:#333; width:88%; margin:40rpx auto 30rpx auto; padding-bottom:30rpx; border-radius:20rpx; text-align:center; box-shadow:3rpx 3rpx 6rpx rgba(0,0,0,0.1); position:relative; }
.container .qrcodeCon .title{ border-bottom:1px dashed #951D3E; padding:20rpx 0; font-size:30rpx;}
.container .qrcodeCon .title .num{ color:#951D3E; font-size:32rpx; margin-top:10rpx;}
.canvas{ height:150rpx; padding:30px 0 15px 0; }
.barcode{ width:500rpx; height:150rpx; margin:0 auto;  }
.container .qrcodeCon .code{ height:320rpx; width:320rpx;}
.container .qrcodeCon .tip{ color:#999; }
.container .qrcodeCon .balance{ font-size:32rpx; line-height:50rpx; height:50rpx;}
.container .qrcodeCon .balance .val{ color:#951D3E; font-size:34rpx; position:relative; padding-left:30rpx; margin-left:30rpx; }
.container .qrcodeCon .balance .val:before{  content:''; position:absolute; left:0; display:block; top:5rpx; height:40rpx; width:2rpx; background:linear-gradient(to bottom,#efefef,#333,#efefef); }
.tag{ position:absolute; bottom:-20rpx; left:50%; margin-left:-20rpx; height:40rpx; width:40rpx; }

.container .swiper { height:360rpx; position:relative; margin-top:110rpx; }
.container .swiper .bg{ height:100%; width:100% }
.container .swiper .swiperCon{ height:350rpx; width:615rpx; border-radius:15rpx; position:absolute; top:50%; left:50%; transform:translate(-50%,-50%); overflow:hidden;}
.container .swiper .swiperGroup{ height:350rpx; }
.container .swiper .swiperGroup .slide{ height:350rpx; width:615rpx;  float:left; position:relative;}
.container .swiper .swiperGroup .slide image{ height:350rpx; width:615rpx; }
.container .swiper .swiperGroup .slide image.avatar{ height:80rpx; width:80rpx; border-radius:50%; position:absolute; top:68rpx; right:40rpx;  }
.container .swiper .swiperGroup .bala{ position:absolute; top:20rpx; right:40rpx; color:#b89249; }
.container .swiper .swiperGroup .bala span{ color:#951e3e;  margin-left:10px; }
.container .swiper .swiperGroup .slide .cardNo{ font-size:32rpx; position:absolute; bottom:35rpx; right:50rpx; }
.container .swiper .swiperGroup .slide .cardType{ font-size:28rpx; position:absolute; bottom:80rpx; right:50rpx; color:#951D3E; }

.container .swiper .swiperGroup .slide .cardNo.c6545{ color:#dddddd; }
.container .swiper .swiperGroup .slide .cardNo.c6546{ color:#dddddd; }
.container .swiper .swiperGroup .slide .cardNo.c6547{ color:#dddddd; }
.container .swiper .swiperGroup .slide .cardNo.c6548{ color:#dddddd; }
.container .swiper .swiperGroup .slide .cardNo.c6549{ color:#fff; }

.container .swiper .swiperGroup .bala.c6547{ color:#7f7f7f; }
.container .swiper .swiperGroup .bala.c6547 span{ color:#fff;  }
.container .swiper .swiperGroup .bala.c6548 span{ color:#fff;  }
.container .swiper .swiperGroup .bala.c6549{ color:#d2a44c; }
.container .swiper .swiperGroup .bala.c6549 span{ color:#fff;  }
.container .swiper .swiperGroup .bala.c6550{ color:#b89249; }
.container .swiper .swiperGroup .bala.c6550 span{ color:#951e3e;  }

.container .swiper .next{ height:40rpx; width:40rpx; position:absolute; right:0; top:50%; transform:translateY(-50%); padding:20rpx; }
.container .swiper .prev{ height:40rpx; width:40rpx; position:absolute; left:0; top:50%; transform:translateY(-50%); padding:20rpx; }
.container .page{ text-align:center; color:#951D3E; font-size:32rpx; border-radius:10rpx; border:1px solid #951D3E; width:150rpx; margin:25rpx auto 0 auto; line-height:50rpx;}
.canvas-box{position:fixed; top:999999rpx; left: 0}


.container .scanIcon{ height:220rpx; width:220rpx; margin:35rpx 0; }
.shadow{ position:fixed; width:100%; height:100%; background:rgba(0,0,0,0.5); z-index:100; }
.qrcodeModal{ text-align:center; position:fixed; top:200rpx; width:80%; left:10%; z-index:1000; background:#fff; border-radius:10px;  }
.qrcodeModal .code{ height:320rpx; width:320rpx;}
.qrcodeModal .tip{ color:#999; }
.noscroll{ overflow:hidden!important; } 
