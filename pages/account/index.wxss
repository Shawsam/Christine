.container .swiper { height:360rpx; position:relative; }
.container .swiper .bg{ height:100%; width:100% }
.container .swiper .swiperCon{height:340rpx; width:540rpx; border-radius:15rpx; position:absolute; top:50%; left:50%; transform:translate(-50%,-50%); overflow:hidden;}
.container .swiper .swiperGroup{ height:340rpx; }
.container .swiper .swiperGroup .slide{ height:340rpx; width:540rpx; float:left; position:relative;}
.container .swiper .swiperGroup .slide image{ height:340rpx; width:540rpx;}
.container .swiper .swiperGroup .slide .cardNo{ font-size:32rpx; position:absolute; bottom:80rpx; right:50rpx; }
.container .swiper .next{ height:40rpx; width:40rpx; position:absolute; right:35rpx; top:50%; margin-top:-20rpx; }
.container .swiper .prev{ height:40rpx; width:40rpx; position:absolute; left:35rpx; top:50%; margin-top:-20rpx; }
.container .page{ text-align:center; color:#951D3E; font-size:32rpx; border-radius:10rpx; border:1px solid #951D3E; width:150rpx; margin:25rpx auto 0 auto; line-height:50rpx;}
