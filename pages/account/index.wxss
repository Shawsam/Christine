page{ background:#FFF5DD; }
.container{  height:100%; overflow-y:scroll; -webkit-overflow-scrolling:touch; box-sizing:border-box; position:relative;}
.container .qrcodeCon{ background:#fff; color:#333; width:88%; margin:40rpx auto 30rpx auto; padding-bottom:50rpx; border-radius:20rpx; text-align:center; box-shadow:3rpx 3rpx 6rpx rgba(0,0,0,0.1); }
.container .qrcodeCon .title{ border-bottom:1px dashed #951D3E; padding:20rpx 0; font-size:30rpx;}
.container .qrcodeCon .title .num{ color:#951D3E; font-size:32rpx; }
.container .qrcodeCon .code{ height:320rpx; width:320rpx; margin:30rpx 0; }
.container .qrcodeCon .tip{ color:#999; }
.container .qrcodeCon .balance{ margin-top:15rpx; font-size:32rpx; }
.container .qrcodeCon .balance .val{ color:#951D3E; font-size:34rpx;}
