page{ background:#FFF5DD; }
page{ background:url('data:image/jpeg;base64,')no-repeat top center; background-size:100% 100%; }
.container{ height:100%; overflow-y:scroll; -webkit-overflow-scrolling:touch; box-sizing:border-box; position:relative; text-align:center; }
.container .topbar{ color:#333; padding:20rpx 0; line-height:40rpx; border-bottom:1px solid #333; }
/*.container .topbar .bar2{ border-left:1px solid #333; }*/
.container .topbar .bar2:before{  content:''; position:absolute; display:block; top:15rpx; height:50rpx; width:2rpx; background:linear-gradient(to bottom,#efefef,#333,#efefef); }

.container .topbar .active{ color:#951D3E; }
.container .swiper { height:450rpx; position:relative; }
.container .swiper .bg{ height:100%; width:100% }
.container .swiper .Oneslide{ position:relative; height:350rpx; width:615rpx; border-radius:18rpx; overflow:hidden; position:absolute; top:50%; left:50%; transform:translate(-50%,-50%);}
.container .swiper .Oneslide .bala{ position:absolute; top:20rpx; right:40rpx; color:#d2a44c; }
.container .swiper .Oneslide .bala span{ color:#fff; margin-left:10px;  }
.container .swiper .Oneslide image{ height:350rpx; width:615rpx; }
.container .swiper .Oneslide image.avatar{ height:80rpx; width:80rpx; border-radius:50%; position:absolute; top:68rpx; right:40rpx;  }
.container .swiper .Oneslide .cardNo{ font-size:32rpx; position:absolute; bottom:35rpx; right:50rpx; color:#fff; }
.container .swiper .Oneslide .cardType{ font-size:28rpx; position:absolute; bottom:80rpx; right:50rpx; color:#951D3E; }


.container .swiper .next{ height:40rpx; width:40rpx; position:absolute; right:0; top:50%; transform:translateY(-50%); padding:20rpx;  }
.container .swiper .prev{ height:40rpx; width:40rpx; position:absolute; left:0; top:50%; transform:translateY(-50%); padding:20rpx;  }
.container .swiper .pagination{ position:absolute; bottom:3rpx; left:50%; transform:translateX(-50%); }
.container .swiper .pagination image{  width:20rpx; height:20rpx; margin:0 10rpx; display:inline-block; vertical-align:middle; margin-top:-5rpx; }
.container .swiper .swiperCon{height:350rpx; width:615rpx; border-radius:18rpx; position:absolute; top:50%; left:50%; transform:translate(-50%,-50%); overflow:hidden;}
.container .swiper .swiperGroup{ height:350rpx; }
.container .swiper .swiperGroup .slide{ height:350rpx; width:615rpx; float:left; position:relative;}
.container .swiper .swiperGroup .slide .bala{ position:absolute; top:20rpx; right:40rpx; color:#b89249; }
.container .swiper .swiperGroup .slide .bala span{ color:#951e3e;  margin-left:10px;  }
.container .swiper .swiperGroup .slide image{ height:350rpx; width:615rpx;}
.container .swiper .swiperGroup .slide .cardNo{ font-size:32rpx; position:absolute; bottom:35rpx; right:50rpx; color:#d9d03b; }
.container .swiper .swiperGroup .slide .cardType{ font-size:28rpx; position:absolute; bottom:80rpx; right:50rpx; color:#951D3E; }

.container .swiper .swiperGroup .cardNo.c6545{ color:#dddddd; }
.container .swiper .swiperGroup .cardNo.c6546{ color:#dddddd; }
.container .swiper .swiperGroup .cardNo.c6547{ color:#dddddd; }
.container .swiper .swiperGroup .cardNo.c6548{ color:#dddddd; }

.container .swiper .swiperGroup .bala.c6547{ color:#7f7f7f; }
.container .swiper .swiperGroup .bala.c6547 span{ color:#fff;  }
.container .swiper .swiperGroup .bala.c6548 span{ color:#fff;  }
.container .swiper .swiperGroup .bala.c6550{ color:#b89249; }
.container .swiper .swiperGroup .bala.c6550 span{ color:#951e3e;  }



.container .text{ color:#666; margin-top:80rpx; font-size:30rpx; }
.container .val{ font-size:40rpx; color:#951D3E; margin-top:10rpx;}
.oneBtn{ padding:0 100rpx; }
.container .btnGroup { margin:70rpx 0 40rpx 0; }
.container .btnGroup .btn{ color:#951D3E; font-size:32rpx; height:50rpx; line-height:50rpx; padding:15rpx 20rpx; text-align:center; border-radius:10rpx;  background:#fff; box-shadow:3rpx 3rpx 6rpx rgba(0,0,0,0.5); }
.container .btnGroup .btn:nth-child(1){ margin:0 60rpx 0 60rpx; }
.container .btnGroup .btn:nth-child(2){ margin:0 60rpx 0 60rpx; }
.container .btnGroup .btn:active{ border:2rpx solid #951D3E; box-shadow:3rpx 3rpx 6rpx rgba(149,29,62,0.5); }
.container .scanIcon{ height:220rpx; width:220rpx; margin:35rpx 0; }
.container .addIcon{ position:absolute; height:113rpx; width:120rpx; bottom:0; right:0 }
